year = int(input("Enter a year: "))

if year % 100 == 0:
    if year % 400 == 0:
        print("In", year, "February has 29 days.")
    else :
        print("In", year, "February has 28 days.")
        
if year % 100 != 0:
    if year % 4 == 0:
        print("In", year, "February has 29 days.")
    else :
        print("In", year, "February has 28 days.")
    
